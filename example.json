{
   "name":"Example",
   "sources":[
      {
         "auth":"none",
         "name":"Source A",
         "endpoints":[
            {
               "id":"custom-source",
               "url":"http://anyboard.io/api/v1/data-custom-format.aspx"
            },
            {
               "id":"general-source",
               "url":"http://anyboard.io/api/v1/data-general-format.aspx"
            }
         ]
      }
   ],
   "dashboards":[
      {
         "name":"Live data",
         "color":"active",
         "source":{
            "endpoint":"custom-source",
            "refresh":5
         },
         "actions":{
            "sound":"sound = false",
            "invoke":"switch = true",
            "invokeTime":"0 0 12 1/1 * ? *"
         },
         "widgets":[
            {
               "width":1,
               "background":"",
               "image":{
                  "url":"http://anyboard.io/img/logo2.png"
               }
            },
            {
               "width":2,
               "type":"none"
            },
            {
               "width":3,
               "height":4,
               "table":{
                  "columns":[
                     {
                        "id":"server",
                        "title":"Server",
                        "style":"bold",
                        "flex":2
                     },
                     {
                        "id":"check",
                        "title":"Last check",
                        "type":"date",
                        "format":"HH:mm:ss"
                     },
                     {
                        "id":"total",
                        "title":"Total",
                        "align":"right"
                     },
                     {
                        "id":"free",
                        "title":"Free %",
                        "style":"bold",
                        "format":".1",
                        "align":"right",
                        "calc":"((total - used) / total) * 100"
                     },
                     {
                        "id":"indicator",
                        "type":"indicator",
                        "colors":{
                           "danger":"free<20",
                           "warning":"free<40"
                        }
                     }
                  ]
               },
               "source":{
                  "endpoint":"general-source",
                  "mapping":{
                     "data[].server":"servers[].server",
                     "data[].check":"servers[].check",
                     "data[].total":"servers[].total",
                     "data[].used":"servers[].used"
                  }
               }
            },
            {
               "type":"basic",
               "source":{
                  "endpoint":"general-source",
                  "mapping":{
                     "subtitle":"processes.3.name",
                     "value":"processes.3.status"
                  }
               }
            },
            {
               "type":"basic",
               "source":{
                  "endpoint":"general-source",
                  "mapping":{
                     "subtitle":"processes[4].name",
                     "value":"processes[4].status"
                  }
               }
            },
            {
               "minitable":{
                  "columns":[
                     {
                        "id":"name",
                        "flex":2
                     },
                     {
                        "id":"value",
                        "style":"bold",
                        "align":"right"
                     }
                  ]
               },
               "source":{
                  "endpoint":"general-source",
                  "mapping":{
                     "data[].name":"processes[].name",
                     "data[].value":"processes[].status"
                  }
               }
            },
            {
               "id":"virus-check",
